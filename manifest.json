{
    "description": "Geoguessr coach",
    "manifest_version": 2,
    "name": "GeoCoach",
    "version": "1.0",
    "homepage_url": "https://github.com/TheGreatRambler/GeoCoach",
    "content_scripts": [
        {
            "matches": [
                "*://*.geoguessr.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "48": "icon.svg",
        "96": "icon.svg"
    }
}